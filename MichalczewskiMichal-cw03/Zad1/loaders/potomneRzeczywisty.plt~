set grid
set term png
set output "../plots/potomneRzeczywisty.png"
set terminal png size 1024,768
set title "Procesy potomne: czas rzeczywisty"
plot '../wyniki/fork.txt' using 1:8 with line title "fork",\
'../wyniki/vfork.txt' using 1:8 with line title "vfork",\
'../wyniki/forkClone.txt' using 1:8 with line title "forkClone",\
'../wyniki/vforkClone.txt' using 1:8 with line title "vforkClone"
replot
